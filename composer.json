{
  "name": "payplug/payplug-woocommerce",
  "description": "",
  "license": "GPL-3.0-or-later",
  "type": "wordpress-plugin",
  "keywords": ["payplug", "woocommerce", "gateway"],
  "authors": [
    {
      "name": "PayPlug",
      "email": "support@payplug.com"
    },
    {
      "name": "Be API",
      "email": "technical@beapi.fr"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "composer/installers": "^1.9",
    "payplug/payplug-php": "^3.3"
  },
  "require-dev": {
    "phpunit/phpunit": "6.5.*",
    "yoast/phpunit-polyfills": "^1.0",
    "phpdocumentor/phpdocumentor": "3.*",
    "bamarni/composer-bin-plugin": "^1.4",
    "automattic/jetpack-changelogger": "3.0.2",
    "squizlabs/php_codesniffer": "*",
    "phpcompatibility/php-compatibility": "*",
    "woocommerce/woocommerce-sniffs": "^0.1.3"
  },
  "autoload": {
    "psr-4": {
      "Payplug\\PayplugWoocommerce\\": "src",
      "libphonenumber\\": "lib/libphonenumber"
    }
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "bamarni/composer-bin-plugin": true,
      "symfony/flex": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
