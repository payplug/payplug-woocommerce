{
    "name": "payplug/payplug-woocommerce",
    "description": "",
    "license": "GPL-3.0-or-later",
    "type": "wordpress-plugin",
    "keywords": ["payplug", "woocommerce", "gateway"],
    "authors": [
        {
            "name": "PayPlug",
            "email": "support@payplug.com"
        },
        {
            "name": "Be API",
            "email": "technical@beapi.fr"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "automattic/jetpack-autoloader": "2.10.1",
        "automattic/jetpack-constants": "1.5.1",
        "composer/installers": "^1.9",
        "payplug/payplug-php": "^3.3"
    },
    "require-dev": {
      "phpunit/phpunit": "6.5.*",
      "yoast/phpunit-polyfills": "^1.0",
      "bamarni/composer-bin-plugin": "^1.4",
      "automattic/jetpack-changelogger": "3.0.2"
    },
    "autoload": {
        "psr-4": {
            "Payplug\\PayplugWoocommerce\\": "src",
            "libphonenumber\\": "lib/libphonenumber"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "automattic/jetpack-autoloader": true,
            "bamarni/composer-bin-plugin": true
        }
    }
}
