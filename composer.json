{
    "name": "payplug/payplug-woocommerce",
    "description": "",
    "license": "GPL-3.0-or-later",
    "type": "wordpress-plugin",
    "keywords": ["payplug", "woocommerce", "gateway"],
    "authors": [
        {
            "name": "PayPlug",
            "email": "support@payplug.com"
        },
        {
            "name": "Be API",
            "email": "technical@beapi.fr"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "composer/installers": "^1.9",
        "payplug/payplug-php": "^3.3"
    },
    "require-dev": {
      "phpunit/phpunit": "7.*",
      "phpdocumentor/phpdocumentor": "2.*",
      "yoast/phpunit-polyfills": "^1.0",
      "10up/wp_mock": "0.4.2",
      "squizlabs/php_codesniffer": "*",
      "phpcompatibility/php-compatibility": "*"
    },
    "autoload": {
        "psr-4": {
            "Payplug\\PayplugWoocommerce\\": "src",
            "libphonenumber\\": "lib/libphonenumber"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}
