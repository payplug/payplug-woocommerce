<?xml version="1.0" encoding="UTF-8"?>
<phpunit
	backupGlobals="false"
	colors="true"
	convertErrorsToExceptions="true"
	convertNoticesToExceptions="true"
	convertWarningsToExceptions="true">
	<testsuites>
		<testsuite name="Payplug">
			<directory suffix=".php">./unit</directory>
		</testsuite>
	</testsuites>
</phpunit>
